{
  "editor.formatOnSave": true,
  "flow.useNPMPackagedFlow": true,
  "javascript.validate.enable": false,
  "typescript.validate.enable": false,
  "files.associations": {
    "*.mdx": "markdown"
  },
  "editor.codeActionsOnSave": {
    "source.organizeImports": false,
    "source.fixAll.eslint": true
  }
}
